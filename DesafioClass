class hero{
    constructor (name  ,idade ,tipo):
        this.nome = nome;
        this.idade = idade;
        this.tipo = tipo;
}

atacar()
    switch(this.tipo){
    case "mago":
        ataque = "usou magia";
      break;

    case "querreiro":
            ataque = "espada";
            break;
    case "monge":
        ataque = "artes marciais";
        break;   

    case "ninja":
    ataque = "shuriken";
    break;
}

console.log(`o ${this.tipo} atacou usando ${ataque}`)
